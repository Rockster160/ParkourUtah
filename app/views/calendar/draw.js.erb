$('.date-holder').html('<%= @date.strftime("%B %Y") %>');
$('.calendar-container').html('<%= j render partial: "calendar/display", locals: {calendar_date: @date, classes: @selected_classes, cities: @selected_cities} %>')
$('.forward-month').attr('href', '/calendar/draw?date=<%= (@date + 1.month) %>')
$('.back-month').attr('href', '/calendar/draw?date=<%= (@date - 1.month) %>')
new loadJS;
